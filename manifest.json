{
  "name": "Motivation",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Motivate you to make the most out of your day!",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["storage"]
}
